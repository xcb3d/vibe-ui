{
  "name": "@vibe-ui/cli",
  "version": "0.1.0",
  "description": "CLI for installing Vibe UI components",
  "type": "module",
  "bin": {
    "vibe-ui": "./dist/index.mjs"
  },
  "main": "dist/index.mjs",
